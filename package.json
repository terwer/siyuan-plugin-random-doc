    "siyuan": "^0.8.8",
    "stylus": "^0.59.0",
    "svelte": "^4.2.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-node-polyfills": "^0.16.0",
