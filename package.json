    "siyuan": "^0.7.2",
    "stylus": "^0.59.0",
    "svelte": "^3.59.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-node-polyfills": "^0.8.2",
